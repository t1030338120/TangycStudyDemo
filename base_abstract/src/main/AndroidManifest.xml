<?xml version="1.0" encoding="utf-8"?>
<manifest package="demo.study.app.com.tangycstudydemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".base_abstract.activity.MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".base_abstract.activity.SencondActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".mvp_loader.MvpLoaderActivity">
        </activity>
        <activity android:name=".AppMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
